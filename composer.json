{
    "type": "library",
    "name": "edmondscommerce/symfony-performance-logger",
    "description": "Provides basic symfony performance logging that's safe to use in a live environment",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "psr/log": "^1.1",
        "symfony/console": "^4.0",
        "symfony/monolog-bundle": "^3.4"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "edmondscommerce/*": "source",
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "EdmondsCommerce\\SymfonyPerformanceLogger\\": ["src/"]
        }
    },
    "minimum-stability": "stable"
}
